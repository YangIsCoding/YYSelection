{
  "name": "yyselection",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3002",
    "build": "prisma generate && next build --no-lint",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "db:seed": "tsx prisma/seed.ts",
    "db:backup": "./scripts/backup-db.sh",
    "db:restore": "./scripts/restore-db.sh",
    "db:status": "node scripts/check-migration-status.js",
    "db:migrate:prod": "npx prisma migrate deploy",
    "db:sync:pull": "tsx scripts/db-sync.ts pull",
    "db:sync:push": "tsx scripts/db-sync.ts push",
    "db:sync:status": "tsx scripts/db-status.ts",
    "db:setup:local": "npx prisma migrate dev --name init && npx prisma db push",
    "db:reset:local": "rm -f prisma/dev.db && npm run db:setup:local",
    "dev:local": "cp .env.local.example .env.local && npm run db:setup:local && npm run dev"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@types/uuid": "^10.0.0",
    "@vercel/blob": "^1.1.1",
    "autoprefixer": "^10.4.21",
    "next": "15.4.5",
    "next-auth": "^4.24.11",
    "picomatch": "^4.0.3",
    "postcss": "^8.5.6",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
